@model IEnumerable<OrenairTraining.Models.testconfig>

@{
    ViewBag.Title = "Index";
}

<h2>Тесты</h2>

<p>
    @Html.ActionLink("Создать новый", "Create", new { themescount = 5 })
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.testconf_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.testconf_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.deleted)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.testconf_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.testconf_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.deleted)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.testconf_id }, new { @class="edit" })
            @Html.ActionLink("Details", "Details", new { id=item.testconf_id },  new { @class="details" })
            @Html.ActionLink("Delete", "Delete", new { id=item.testconf_id },  new { @class="delete" })
        </td>
    </tr>
}

</table>
