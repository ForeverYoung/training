@model IEnumerable<OrenairTraining.Models.testconfig>

@{
    ViewBag.Title = "Index";
}

<h2>Тесты</h2>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.testconf_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.testconf_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.deleted)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.testconf_id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.testconf_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.deleted)
        </td>
        <td>
            @Html.ActionLink("Start", "StartTest", new { config_id = item.testconf_id }, new { @class="starttest" })
        </td>
    </tr>
}

</table>
