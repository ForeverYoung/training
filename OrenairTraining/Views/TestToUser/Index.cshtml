@model IEnumerable<OrenairTraining.Models.testtouser>

@{
    ViewBag.Title = "Index";
}

<h2>Непройденные тесты</h2>

@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<table>
    <tr>
        <th>
            @Html.DisplayName("Пользователь")
        </th>
        <th>
            @Html.DisplayName("Тест")
        </th>
        <th>
            @Html.DisplayName("Завершен")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.Label((ViewBag.Users as List<OrenairTraining.Models.user>).FirstOrDefault(t => t.user_id == item.user_id).user_name)
        </td>
        <td>
            @Html.Label((ViewBag.Configs as List<OrenairTraining.Models.testconfig>).FirstOrDefault(t => t.testconf_id == item.testconf_id).testconf_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.is_completed)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
